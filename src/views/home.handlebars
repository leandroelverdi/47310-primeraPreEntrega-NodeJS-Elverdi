<main>
  <section class="home__section">
    <h1 class="home__title">local list</h1>

    <table class="table" id="table">
      <thead>
        <tr>
          <th>id</th>
          <th>title</th>
          <th>price</th>
          <th>description</th>
        </tr>
      </thead>
      <tbody id="tableBody">
        {{#each products}}
          <tr>
            <td>{{id}}</td>
            <td>{{title}}</td>
            <td>${{price}}</td>
            <td>{{description}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </section>
</main>